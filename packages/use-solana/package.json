  "devDependencies": {
    "@size-limit/file": "^4.12.0",
    "@size-limit/webpack": "^4.12.0",
    "@solana/web3.js": "^1.18.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.12.0",
    "tsdx": "^0.14.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@ledgerhq/hw-transport": "^5.51.1",
    "@ledgerhq/hw-transport-webusb": "^5.53.1",
    "@project-serum/sol-wallet-adapter": "^0.2.0",
    "@types/bs58": "^4.0.1",
    "@types/ledgerhq__hw-transport": "^4.21.3",
    "@types/ledgerhq__hw-transport-webusb": "^4.70.1",
    "bs58": "^4.0.1",
    "eventemitter3": "^4.0.7",
    "unstated-next": "^1.1.0"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.15.0",
    "@types/react": "^17.0.8",
    "react": ">=16"
  },
  "main": "dist/use-solana.cjs.production.min.js",
  "types": "dist/index.d.ts",
